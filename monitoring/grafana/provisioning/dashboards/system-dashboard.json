{
  "title": "Moonshine System Resources",
  "uid": "moonshine-system",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "Container CPU Usage",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "datasource": {"type": "prometheus"},
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{name=~\"moonshine.*\"}[1m]) * 100",
          "legendFormat": "{{name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100,
          "min": 0
        }
      }
    },
    {
      "id": 2,
      "title": "Container Memory Usage",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "datasource": {"type": "prometheus"},
      "targets": [
        {
          "expr": "container_memory_usage_bytes{name=~\"moonshine.*\"}",
          "legendFormat": "{{name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "id": 3,
      "title": "Network I/O",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "datasource": {"type": "prometheus"},
      "targets": [
        {
          "expr": "rate(container_network_receive_bytes_total{name=~\"moonshine.*\"}[1m])",
          "legendFormat": "RX {{name}}"
        },
        {
          "expr": "rate(container_network_transmit_bytes_total{name=~\"moonshine.*\"}[1m])",
          "legendFormat": "TX {{name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps"
        }
      }
    },
    {
      "id": 4,
      "title": "Disk I/O",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "datasource": {"type": "prometheus"},
      "targets": [
        {
          "expr": "rate(container_fs_reads_bytes_total{name=~\"moonshine.*\"}[1m])",
          "legendFormat": "Read {{name}}"
        },
        {
          "expr": "rate(container_fs_writes_bytes_total{name=~\"moonshine.*\"}[1m])",
          "legendFormat": "Write {{name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps"
        }
      }
    }
  ],
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timezone": "browser",
  "schemaVersion": 38
}
